<div class="col-md-10 center">

<table>
  <thead>
    <tr>
      <th>First Name</th>
      <th>Last Price</th>
      <th>Title</th>
    </tr>
  </thead>
  <tbody>
    <% @officers.each do |officer| %>
      <tr>
        <td><%= officer.first %></td>
        <td><%= officer.last %></td>
        <td><%= officer.title %></td>
      </tr>
    <% end %>
  </tbody>
</table>
<div>

<h3>Import a CSV File</h3>
  <%= form_tag import_loan_officers_path, multipart: true do %>
    <%= file_field_tag :file %>
    <%= submit_tag "Submit" %>
  <% end %>
</div>

</div>