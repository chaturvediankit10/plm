<div class="main-form panel">
  <div class="panel-heading">Program Inputs</div>
  <div class="panel-body">
    <div class="row">
      <div class="col-md-5">
        <div class="program-panel panel-basic">
          <div class="input-grp"><h5>Program Info</h5></div>
          <div class="row panel-body">
            <div class="col-md-6">
              <div class="form-group">
                <label>Bank Name</label>
                <%= select_tag 'bank_name', options_for_select(@all_banks_name.sort_by!{ |m| m.downcase }, params[:bank_name]), { :class=> "form-control" ,:include_blank => 'All'} %>
              </div>
            </div>
            <div class="col-md-6">
              <div class="form-group">
                <label>Loan Category</label>
                <%= select_tag 'loan_category', options_for_select(@loan_categories.sort_by!{ |m| m.downcase }, params[:loan_category]), { :class=> "form-control" ,:include_blank => 'All'} %>
              </div>
            </div>
            <div class="col-md-6">
              <div class="form-group">
                <label>Program Category</label>
                <%= select_tag 'pro_category', options_for_select(@program_categories, params[:pro_category]), { :class=> "form-control"} %>
              </div>
            </div>
            <div class="col-md-6">
              <div class="form-group">
                <label>Program Name</label>
                <%= select_tag 'program_name', options_for_select(@program_names, params[:program_name]), { :include_blank => 'All', :class=> "form-control"} %>
              </div>
            </div>
          </div>
        </div>
      </div>
      <div class="col-md-7">
        <div class="panel-basic">
          <div class="input-grp"><h5>Loan Property</h5></div>
          <div class="row panel-body">
            <div class="row col-md-12">
              <div class="col-md-6">
                <div class="form-group">
                  <label>Loan Purpose</label>
                    <%= select_tag 'loan_purpose', options_for_select(Program::LOAN_PURPOSE.flatten.sort_by!{ |m| m.downcase }, (params[:loan_purpose] rescue nil)), { prompt: 'Select Loan Purpose', :class=> "form-control" }%>
                  <span class="error-loan-purpose">Please Select Refinance</span>
                </div>
              </div>
              <div class="col-md-6">
                <div class="form-group">
                  <label>Loan Size</label>
                    <%= select_tag 'loan_size', options_for_select(Program::LOAN_SIZE.flatten.sort_by!{ |m| m.downcase }, (params[:loan_size] rescue nil)), { prompt: 'Select Loan Size', :class=> "form-control" }%>
                </div>
              </div>
            </div>
            <div class="row col-md-12">
              <div class="col-md-6">
                <div class="form-group">
                  <label>Loan Type</label>
                    <%= select_tag 'loan_type', options_for_select(Program::LOAN_TYPE.flatten.sort_by!{ |m| m.downcase }, (params[:loan_type] rescue nil)), { prompt: 'Select Loan Type', :class=> "form-control" } %>
                </div>
              </div>
              <div class="col-md-6 term">
                <div class="form-group">
                  <label>Term</label>
                  <div class="input-group">
                    <%= select_tag 'term', options_for_select(@term_list, (params[:term] rescue nil)), { prompt: 'Select Term', :class=> "form-control" } %>
                  </div>
                </div>
              </div>
              <div class="col-md-6 arm-basic">
                <div class="form-group">
                  <label>ARM Basic</label>
                  <div class="input-group">
                    <%= select_tag 'arm_basic', options_for_select(Program::ARM_BASIC.flatten, (params[:arm_basic].present? ? params[:arm_basic] : "10/1 ARM")), { prompt: 'Select ARM Basic', :class=> "form-control"} %>
                  </div>
                </div>
              </div>
              <div class="col-md-6 arm-basic">
                <div class="form-group">
                  <label>ARM Benchmark</label>
                  <%= select_tag 'arm_benchmark', options_for_select(Program::ARM_BENCHMARK_LIST.flatten.sort_by!{ |m| m.downcase }, (params[:arm_benchmark] rescue nil)), { prompt: 'Select ARM Benchmark', :class=> "form-control" } %>
                </div>
              </div>
              <div class="col-md-6 arm-basic">
                <div class="form-group">
                  <label>ARM Margin</label>
                  <%= select_tag 'arm_margin', options_for_select(Program::ARM_MARGIN_LIST.flatten, (params[:arm_margin] rescue nil)), { prompt: 'Select ARM Margin', :class=> "form-control"} %>
                </div>
              </div>
              <div class="col-md-6 arm-basic">
                <div class="form-group">
                  <label>ARM Advanced</label>
                  <%= select_tag 'arm_advanced', options_for_select( @arm_advanced_list.flatten.sort_by!{ |m| m.downcase }.prepend(["All"]), (params[:arm_advanced] rescue nil)), { prompt: 'Select ARM Advanced', :class=> "form-control"} %>
                </div>
              </div>
            </div>
            <div class="row col-md-12">
              <div class="col-md-6">
                <div class="form-group">
                  <label>Interest Rate</label>
                  <%= select_tag 'interest', options_for_select(Program::INTEREST_LIST, @interest), :class=> "form-control" %>
                </div>
              </div>
              <div class="col-md-6">
                <div class="form-group">
                  <label>Lock Period</label>
                  <div class="input-group">
                    <%= select_tag 'lock_period', options_for_select(Program::LOCK_PERIOD_LIST, @lock_period), :class=> "form-control" %>
                  </div>
                </div>
              </div>
            </div>
          </div>
        </div>
      </div>
    </div>
    <div class="row">
      <div class="col-md-5">
        <div class="program-panel panel-basic">
          <div class="input-grp"><h5>Government Program</h5></div>
          <div class="row panel-body">
            <div class="col-md-12">
              <div class="row">
                <div class="col-md-4">
                  <div class="form-group">
                    <div class="checkbox">
                      <label><%= check_box_tag 'streamline', 'streamline', params[:streamline].present? %> Streamline</label>
                    </div>
                  </div>
                </div>
                <div class="col-md-4">
                  <div class="form-group">
                    <div class="checkbox">
                      <label> <%= check_box_tag 'fha', 'fha', params[:fha].present? %> FHA</label>
                    </div>
                  </div>
                </div>
              </div>
              <div class="row">
                <div class="col-md-12">
                  <div class="form-group">
                    <div class="checkbox">
                      <label><%= check_box_tag 'va', 'va', params[:va].present? %> VA</label>
                    </div>
                  </div>
                </div>
              </div>
              <div class="row">
                <div class="col-md-12">
                  <div class="form-group">
                    <div class="checkbox">
                      <label><%= check_box_tag 'usda', 'usda', params[:usda].present? %> USDA</label>
                    </div>
                  </div>
                </div>
              </div>
            </div>
          </div>
        </div>
      </div>
      <div class="col-md-7">
        <div class="program-panel panel-basic">
          <div class="input-grp"><h5>FM</h5></div>
          <div class="row panel-body">
            <div class="row col-md-12">
              <div class="col-md-4">
                <div class="checkbox">
                  <label> <%= check_box_tag 'fannie_mae', 'fannie_mae', params[:fannie_mae].present? %> Fannie Mae</label>
                </div>
              </div>
              <div class="col-md-4">
                <div class="checkbox">
                  <label> <%= check_box_tag 'du', 'du', params[:du].present? %> DU</label>
                </div>
              </div>
              <div class="col-md-4">
                <div class="form-group">
                  <%= select_tag 'fannie_mae_product', options_for_select(Program::FANNIE_MAE_PRODUCT_LIST.flatten.sort_by!{ |m| m.downcase }, (params[:fannie_mae_product] rescue nil)), { prompt: 'Select Product', :class=> "form-control" } %>
                </div>
              </div>
            </div>
            <div class="row col-md-12">
              <div class="col-md-4">
                <div class="checkbox">
                  <label> <%= check_box_tag 'freddie_mac', 'freddie_mac', params[:freddie_mac].present? %> Freddie Mac</label>
                </div>
              </div>
              <div class="col-md-4">
                <div class="checkbox">
                  <label> <%= check_box_tag 'lp', 'lp', params[:lp].present? %> LP</label>
                </div>
              </div>
              <div class="col-md-4">
                <div class="form-group">
                  <div class="form-group">
                    <%= select_tag 'freddie_mac_product', options_for_select(Program::FREDDIE_MAC_PRODUCT_LIST.flatten.sort_by!{ |m| m.downcase }, (params[:freddie_mac_product] rescue nil)), { prompt: 'Select Product', :class=> "form-control" } %>
                  </div>
                </div>
              </div>
            </div>
          </div>
        </div>
      </div>
    </div>
    <div class="row">
      <div class="col-md-2">
        <div class="form-group">
          <div class="checkbox">
            <label><%= check_box_tag 'full_doc', 'full_doc', params[:full_doc].present? %> Full Doc</label>
          </div>
        </div>
      </div>
    </div>
  </div>
</div>
<div class="main-form panel">
  <div class="panel-heading">Adjustment Inputs</div>
  <div class="panel-body">
    <div class="row">
      <div class="col-md-2">
        <div class="form-group">
          <label>Credit Score</label>
          <div class="input-group">
            <%= select_tag 'credit_score', options_for_select(Program::CREDIT_SCORE_LIST, (params[:credit_score].present? ? params[:credit_score] : "700-719")), :class=> "form-control" %>
          </div>
        </div>
      </div>
      <div class="col-md-2">
        <div class="form-group">
          <label>LTV</label>
          <div class="input-group">
            <%= select_tag 'ltv', options_for_select(Program::LTV_VALUES, (params[:ltv].present? ? params[:ltv] : "65.01 - 70.00")), :class=> "form-control" %>
          </div>
        </div>
      </div>
      <div class="col-md-2">
        <div class="form-group">
          <label>CLTV</label>
          <div class="input-group">
            <%= select_tag 'cltv', options_for_select(Program::CLTV_VALUES, (params[:cltv].present? ? params[:cltv] : "75.01 - 80.00")), :class=> "form-control" %>
          </div>
        </div>
      </div>
      <div class="col-md-2">
        <div class="form-group">
          <label>State</label>
          <div class="input-group">
            <%= select_tag 'state', options_for_select(Program::STATE, (params[:state] rescue nil)), :class=> "form-control" %>
          </div>
        </div>
      </div>
      <div class="col-md-2">
        <div class="form-group">
          <label>Loan Amount</label>
          <% @loan_amount_list =%>
          <%= select_tag 'loan_amount', options_for_select(Program::LOAN_AMOUNT, (params[:loan_amount] rescue nil)), { :class=> "form-control", :required => true} %>
        </div>
      </div>
      <div class="col-md-2">
        <div class="form-group">
          <label>Program Category</label>
          <%= select_tag 'program_category', options_for_select(Program::PROGRAM_CATEGORY_LIST.sort, (params[:program_category] rescue nil)), { :class=> "form-control", :required => true} %>
        </div>
      </div>
      <div class="col-md-2">
        <div class="form-group">
          <label>Property Type</label>
          <div class="input-group">
            <%= select_tag 'property_type', options_for_select(Program::PROPERTY_TYPE_VALUES.sort, (params[:property_type] rescue nil)), :class=> "form-control" %>
          </div>
        </div>
      </div>
      <div class="col-md-2">
        <div class="form-group">
          <label>Financing Type</label>
          <div class="input-group">
            <%= select_tag 'financing_type', options_for_select(Program::FINANCING_TYPE_VALUES.sort, (params[:financing_type] rescue nil)), :class=> "form-control" %>
          </div>
        </div>
      </div>
      <div class="col-md-2">
        <div class="form-group">
          <label>Refinance Option</label>
          <div class="input-group">
            <%= select_tag 'refinance_option', options_for_select(Program::REFINANCE_OPTION_VALUES.sort, (params[:refinance_option] rescue nil)), :class=> "form-control" %>
          </div>
        </div>
      </div>
      <div class="col-md-2">
        <div class="form-group">
          <label>Misc Adjuster</label>
          <div class="input-group">
            <%= select_tag 'misc_adjuster', options_for_select(Program::MISC_ADJUSTER_VALUES.sort, (params[:misc_adjuster] rescue nil)), :class=> "form-control" %>
          </div>
        </div>
      </div>
      <div class="col-md-2">
        <div class="form-group">
          <label>Payment Type</label>
          <div class="input-group">
            <%= select_tag 'payment_type', options_for_select(Program::PATMENT_TYPE_VALUES.sort, (params[:payment_type] rescue nil)), :class=> "form-control" %>
          </div>
        </div>
      </div>
      <div class="col-md-2">
        <div class="form-group">
          <label>DTI</label>
          <div class="input-group">
            <%= select_tag 'dti', options_for_select(Program::DTI_VALUES.sort, (params[:dti] rescue nil)), :class=> "form-control" %>
          </div>
        </div>
      </div>
      <div class="col-md-2">
        <div class="form-group">
          <label>Coverage</label>
          <div class="input-group">
            <%= select_tag 'coverage', options_for_select(Program::COVERAGE_VALUES.sort, (params[:coverage] rescue nil)), :class=> "form-control" %>
          </div>
        </div>
      </div>
      <div class="col-md-2">
        <div class="form-group">
          <label>Margin</label>
          <div class="input-group">
            <%= select_tag 'margin', options_for_select(Program::MARGIN_VALUES.sort, (params[:margin] rescue nil)), :class=> "form-control" %>
          </div>
        </div>
      </div>
    </div>
  </div>
</div>
<div class="row">
  <div class="col-md-8"></div>
  <div class="col-md-4">
    <div class="form-group">
      <div>
        <label></label>
      </div>
      <%#= submit_tag "Search", :class => "btn btn-info btn-block" %>
    </div>
  </div>
</div>