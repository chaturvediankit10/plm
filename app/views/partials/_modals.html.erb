<!-- advance search mobile start-->
 <div class="modal fade" id="advance-srch-mb" role="dialog">
    <div class="modal-dialog">
       <div class="modal-content">
          <div class="modal-header">
             <button type="button" class="close" data-dismiss="modal">&times;</button>
             <h4 class="modal-title">Advanced Options</h4>
             <a href="">Reset</a>
          </div>
          <div class="modal-body">
             <div class="search-main adv-search-mb">
                <div class="search-cat zip-srch">
                   <label>Zip Code</label>
                   <input placeholder="123456" type="text">
                </div>
                <div class="search-cat purpse-srch">
                   <label>Purpose</label>
                   <select>
                      <option>Purchase</option>
                   </select>
                </div>
                <div class="search-cat price-srch">
                   <label>Home Price</label>
                   <div class="dollar-icn">$</div>
                   <input placeholder="123456" type="text">
                </div>
                <div class="search-cat price-srch">
                   <label>Down Payment</label>
                   <div class="dollar-icn">$</div>
                   <input placeholder="123456" type="text">
                </div>
                <div class="search-cat cr-srch">
                   <label>Credit Score</label>
                   <select>
                      <option>750+</option>
                   </select>
                </div>
                <div class="search-adv-option-box">
                   <div class="search-adv-option">
                      <div class="row">
                         <h4>desired loan programs?</h4>
                         <div class="col-xs-12 col-md-2 advance-check">
                            <div class="form-group"><input value="" type="checkbox"> 30 Year Fixed</div>
                            <div class="form-group"><input value="" type="checkbox"> 20 Year Fixed</div>
                            <div class="form-group"><input value="" type="checkbox"> 15 Year Fixed</div>
                            <div class="form-group"><input value="" type="checkbox"> 10 Year Fixed</div>
                            <div class="form-group"><input value="" type="checkbox"> 7/1 Arm</div>
                            <div class="form-group"><input value="" type="checkbox"> 5/1 Arm</div>
                            <div class="form-group"><input value="" type="checkbox"> 3/1 Arm</div>
                         </div>
                         <div class="col-xs-12 col-md-3 drpdown-box">
                            <select>
                               <option>Select Occupancy</option>
                            </select>
                            <select>
                               <option>Select Occupancy</option>
                            </select>
                            <select>
                               <option>Select Occupancy</option>
                            </select>
                            <select>
                               <option>Select Occupancy</option>
                            </select>
                         </div>
                         <div class="col-xs-12 col-md-4 drpdown-box">
                            <select>
                               <option>Select Prepay Penalty</option>
                            </select>
                            <label>Cash Reserves (mos)</label>
                            <select>
                               <option>$ 15</option>
                            </select>
                            <label>Monthly Debt</label>
                            <select>
                               <option>$ 0</option>
                            </select>
                            <label>Monthly Income</label>
                            <select>
                               <option>$ 5,000</option>
                            </select>
                         </div>
                         <div class="col-xs-12 col-md-3 advance-check top-bordr-only">
                            <p>Owned Properties <span>01</span></p>
                            <div class="form-group"><input value="" type="checkbox"> Interest Only</div>
                            <div class="form-group"><input value="" type="checkbox"> Hazard Insuarance Waived</div>
                            <div class="form-group"><input value="" type="checkbox"> Property Taxes Waived</div>
                            <div class="form-group"><input value="" type="checkbox"> First Time Home Buyer</div>
                            <div class="form-group"><input value="" type="checkbox"> Non Perm Resident Alien</div>
                            <div class="form-group"><input value="" type="checkbox"> Foreign National</div>
                            <div class="form-group"><input value="" type="checkbox"> Miltary</div>
                            <div class="form-group"><input value="" type="checkbox"> Rular</div>
                         </div>
                      </div>
                   </div>
                </div>
             </div>
          </div>
          <div class="modal-footer">
             <button class="rates-btn">Get Rates</button>
          </div>
       </div>
    </div>
 </div>
	<!-- advance search mobile end-->
	<!-- Login popup start-->
	 <div class="modal fade signup-login-popup login-popup" id="login-popup" role="dialog">
          <div class="modal-dialog">
             <div class="modal-content">
                <div class="modal-header">
                   <button type="button" class="close" data-dismiss="modal"><%= image_tag("Homepage-login-overlayw_03.png", :alt => "close", :class => "login-cross-img") %></button>
                </div>
                <div class="modal-body">
						<%= form_for(:user, :url => session_path(:user), :html => {:id => "login_form"}) do |f| %>
						 <ul class="login-drpdwn">
                               <li>
								 <%= image_tag("login-mail.png", :alt => "login-mail") %>
								 <%= f.email_field :email, autofocus: true %>
							</li>
                               <li>
								 <%= image_tag("login-pass.png", :alt => "login-pass") %>
								 <%= f.password_field :password, autocomplete: "off" %>
							</li>

               <li class="cntnt-login">
                  <button class="rates-btn">Log In</button>
								<div class="forgot-signup-links">
                     <%= link_to "Forgot Password", new_user_password_path %>
                     or
                     <a href="" data-toggle="modal" data-target="#signup-popup" class="signup-link-clr">Sign up Now</a>
                  </div>
                  <p>or sign up with</p>
                  <div class="social-login">
                     <a href=""><%= image_tag("facebook_13.png", :alt => "Facebook") %></a>
                     <%#= link_to "Sign in with Facebook", user_facebook_omniauth_authorize_url() %>
                     <a href=""><%= image_tag("twitter_15.png", :alt => "Twitter") %></a>
                     <a href=""><%= image_tag("google_13.png", :alt => "Google") %></a>
                  </div>
               </li>
            </ul>
            <% end %>
                </div>
             </div>
          </div>
       </div>
 	<!-- login popup end-->
 	<!-- signup popup start-->
	<div class="modal fade signup-login-popup" id="signup-popup" role="dialog">
      <div class="modal-dialog">
         <div class="modal-content">
            <div class="modal-header">
               <button type="button" class="close" data-dismiss="modal"><%= image_tag("Homepage-login-overlayw_03.png", :alt => "close", :class => "login-cross-img") %> </button>
            </div>
            <div class="modal-body">
  						<%= form_for(:user, url: user_registration_path, :html => {:id => "signup_form"}) do |f| %>
  						 <ul class="login-drpdwn">
                        <li>
  								 <%= image_tag("login-user.png", :alt => "login-user") %>
  								 <%= f.text_field :first_name, autofocus: true, placeholder: "First Name" %>
  							</li>
                           <li>
  								 <%= image_tag("login-mail.png", :alt => "login-mail") %>
  								 <%= f.email_field :email, autofocus: true, placeholder: "Email" %>
  								</li>
                                 <li>
  								 <%= image_tag("login-pass.png", :alt => "login-pass") %>
  								 <%= f.password_field :password, autocomplete: "off", placeholder: "Password" %>
  							</li>
  							<li>
  								 <%= image_tag("login-pass.png", :alt => "login-pass") %>
  								 <%= f.password_field :password_confirmation, autocomplete: "off", placeholder: "Confirm Password" %>
  							</li>
                 <li class="cntnt-login">
                    <button class="rates-btn">Sign Up</button>
                    <p>or sign up with</p>
                    <div class="social-login">
                       <a href=""><%= image_tag("facebook_13.png", :alt => "Facebook") %></a>
                       <a href=""><%= image_tag("twitter_15.png", :alt => "Twitter") %></a>
                       <a href=""><%= image_tag("google_13.png", :alt => "Google") %></a>
                    </div>
                    <span>Have an account? <a href="" class="setLogin" data-toggle="modal" data-target="#login-popup">Login</a></span>
                 </li>
              </ul>
              <% end %>
            </div>
         </div>
      </div>
  </div>
	<!-- signup popup end-->
