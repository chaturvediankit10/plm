<!-- Login popup start-->
<div class="modal fade signup-login-popup login-popup modal-bx" id="login-popup" role="dialog">
  <div class="modal-dialog">
   <div class="modal-content">
    <div class="modal-header login-mdl-header">
     <h4>Login</h4>
     <button type="button" class="modal-close" data-dismiss="modal">
      <i class="fa fa-times"></i>
     </button>
    </div>
    <div class="modal-header price-alert-header" style="display: none;">
       <button type="button" class="modal-close" data-dismiss="modal"><i class="fa fa-times"></i></button>
       <h3>Create a Price Alert to obtain lowest Mortgage
          <br>Rates and Save even more!
       </h4>
    </div>
    <div class="modal-body">
      <%= form_for(:user, :url => session_path(:user), :html => {:id => "login_form"}) do |f| %>
        <ul class="login-drpdwn">
          <li>
            <%#= image_tag("login-mail.png", :alt => "login-mail") %>
            <i class="form-icon fa fa-envelope-o"></i>
            <%= f.email_field :email, autofocus: true, placeholder: "Email" %>
          </li>
          <li>
            <%#= image_tag("login-pass.png", :alt => "login-pass") %>
            <i class="form-icon fa fa-key"></i>
            <%= f.password_field :password, autocomplete: "off", placeholder: "Password" %>
          </li>
          <li class="cntnt-login">
            <button class="rates-btn">Log In</button>
            <div class="forgot-signup-links">
              <a href="" class="setForgot" data-toggle="modal" data-target="#forgot-popup">Forgot Password</a>
              <a href="" data-toggle="modal" data-target="#signup-popup" class="signup-link-clr">Sign up Now</a>
            </div>
            <div class="divider">
              <span>or sign up with</span>
            </div>
             <div class="social-login">

              <%#= link_to image_tag("facebook_13.png"), user_facebook_omniauth_authorize_url() %>
              <%= link_to user_facebook_omniauth_authorize_url(), class: "btn facebook-btn" do %>
                <i class="fa fa-facebook"></i> Facebook
              <% end %>
              <!-- <a href=""><%#= image_tag("twitter_15.png", :alt => "Twitter") %></a> -->
              <!-- <a href=""><%#= image_tag("google_13.png", :alt => "Google") %></a> -->
              <%= link_to user_google_oauth2_omniauth_authorize_url(), class: "btn google-plus-btn" do %>
                <i class="fa fa-google-plus"></i> Gmail
              <% end %>
              <%#= link_to image_tag("google_13.png"), user_google_oauth2_omniauth_authorize_url() %>
             </div>
          </li>
         </ul>
       <% end %>
      </div>
    </div>
  </div>
</div>
<!-- login popup end-->

<!-- forgot password  popup start-->
<div class="modal fade signup-login-popup login-popup" id="forgot-popup" role="dialog">
  <div class="modal-dialog">
   <div class="modal-content">
    <div class="modal-header login-mdl-header">
     <h4>Reset Password</h4>
     <button type="button" class="modal-close" data-dismiss="modal">
      <i class="fa fa-times"></i>
     </button>
    </div>
    <div class="modal-body">
      <%= form_for(resource, as: resource_name, url: password_path(resource_name), html: { method: :post, id: 'forgot_signup_form_pop' }) do |f| %>
        <ul class="login-drpdwn">
          <li>
            <%#= image_tag("login-mail.png", :alt => "login-mail") %>
            <i class="form-icon fa fa-envelope-o"></i>
            <%= f.email_field :email, autofocus: true, placeholder: "Email" %>
          </li>
          <li class="cntnt-login">
            <button class="rates-btn">Submit</button>
            <div class="forgot-signup-links">
              <a href="" class="setLogin" data-toggle="modal" data-target="#login-popup">Login</a>
              <a href="" data-toggle="modal" data-target="#signup-popup" class="signup-link-clr">Sign up Now</a>
            </div>
            <div class="divider">
              <span>or sign up with</span>
            </div>

             <div class="social-login">
              <!-- <a href=""><%#= image_tag("facebook_13.png", :alt => "Facebook") %></a> -->

              <%= link_to user_facebook_omniauth_authorize_url(), class: "btn facebook-btn" do %>
                <i class="fa fa-facebook"></i> Facebook
              <% end %>

              <%#= link_to image_tag("facebook_13.png"), user_facebook_omniauth_authorize_url() %>
              <!-- <a href=""><%#= image_tag("twitter_15.png", :alt => "Twitter") %></a> -->
              <!-- <a href=""><%#= image_tag("google_13.png", :alt => "Google") %></a> -->

              <%= link_to user_google_oauth2_omniauth_authorize_url(), class: "btn google-plus-btn" do %>
                <i class="fa fa-google-plus"></i> Gmail
              <% end %>
              <%#= link_to image_tag("google_13.png"), user_google_oauth2_omniauth_authorize_url() %>
             </div>
          </li>
         </ul>
       <% end %>
      </div>
    </div>
  </div>
</div>
<!-- forgot password popup end-->


<!-- signup popup start-->
<div class="modal fade signup-login-popup modal-bx" id="signup-popup" role="dialog">
  <div class="modal-dialog">
    <div class="modal-content">
      <div class="modal-header login-mdl-header">
        <h4>Sign Up</h4>
         <button type="button" class="modal-close" data-dismiss="modal">
           <i class="fa fa-times"></i>
         </button>
      </div>
      <div class="modal-header" style="display: none;">
         <button type="button" class="modal-close" data-dismiss="modal">
           <i class="fa fa-times"></i>
         </button>
         <h4>Create a Price Alert to obtain lowest Mortgage</br>
            Rates and Save even more!
         </h4>
      </div>
      <div class="modal-body">
        <%= form_for(:user, url: user_registration_path, :html => {:id => "signup_form"}) do |f| %>
         <ul class="login-drpdwn">
           <li>
             <%#= image_tag("login-user.png", :alt => "login-user") %>
             <i class="form-icon fa fa-user"></i>
             <%= f.text_field :first_name, autofocus: true, placeholder: "First Name" %>
           </li>
           <li>
             <%#= image_tag("login-user.png", :alt => "login-user") %>
             <i class="form-icon fa fa-user"></i>
             <%= f.text_field :last_name, autofocus: true, placeholder: "Last Name" %>
           </li>
           <li>
             <%#= image_tag("login-mail.png", :alt => "login-mail") %>
             <i class="form-icon fa fa-envelope-o"></i>
             <%= f.email_field :email, autofocus: true, placeholder: "Email" %>
           </li>
           <li>
             <%#= image_tag("login-pass.png", :alt => "login-pass") %>

             <i class="form-icon fa fa-key"></i>
             <%= f.password_field :password, autocomplete: "off", placeholder: "Password" %>
           </li>
           <li>
             <%#= image_tag("login-pass.png", :alt => "login-pass") %>
             <i class="form-icon fa fa-key"></i>
             <%= f.password_field :password_confirmation, autocomplete: "off", placeholder: "Confirm Password" %>
           </li>
           <li class="cntnt-login">
             <button class="rates-btn">Sign Up</button>
             <div class="divider">
              <span>or sign up with</span>
            </div>
             <div class="social-login">

              <%= link_to user_facebook_omniauth_authorize_url(), class: "btn facebook-btn" do %>
                <i class="fa fa-facebook"></i> Facebook
              <% end %>

              <%= link_to user_google_oauth2_omniauth_authorize_url(), class: "btn twitter-btn" do %>
                <i class="fa fa-twitter"></i> Twitter
              <% end %>

              <%= link_to user_google_oauth2_omniauth_authorize_url(), class: "btn google-plus-btn" do %>
                <i class="fa fa-google-plus"></i> Gmail
              <% end %>


               <!-- <a href=""><%#= image_tag("facebook_13.png", :alt => "Facebook") %></a>
               <a href=""><%#= image_tag("twitter_15.png", :alt => "Twitter") %></a>
               <a href=""><%#= image_tag("google_13.png", :alt => "Google") %></a> -->
             </div>
            <span>Have an account? <a href="" class="setLogin" data-toggle="modal" data-target="#login-popup">Login</a></span>
           </li>
         </ul>
      <% end %>
     </div>
   </div>
  </div>
</div>
<!-- signup popup end-->

