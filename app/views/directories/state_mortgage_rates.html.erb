 <!-- dirctory mortgage rates in state start-->
         <section class="content-main dirctory mortgage-rates-state">
            <div class="container">
               <div class="directory-box">
                  <h1>Cities in <%=state_full_name(@state_name)%></h1>
                     <div class="directory-links-box">
                        <%@cities.each_slice(17) do |city|%>
                           <div class="col-xs-12 col-md-4">
                              <%city.each do |c|%>               
                              <%=link_to c,c%>
                              <%end%>
                           </div>      
                        <%end%>   
                     </div>
                  <%if @cities.present? && @cities.count == 50%>
                     <div class="col-xs-12 col-md-4">
                        <%= form_tag({controller: "/directories",action: "state_mortgage_rates"},method: :get) do |f|%>
                            <%=hidden_field_tag "state", params[:state]%>
                            <%=hidden_field_tag "lc", @cities.last%>
                           <%= submit_tag 'Lists of Cities', class: 'load-more-list'%>
                        <%end%>
                     </div>
                  <%else%>  
                    <%= link_to "Back", :back %>
                  <%end%> 
