<!-- Work on progress -->
<h4> Profile </h4>

<%= form_with url: profile_path do |form| %>
  <%= form.label :email%>
  <%= form.text_field :email, value: @user.email %><br/>
  <%= form.label :zip_code%>
  <%= form.text_field :zip_code, value: @user.zip_code %><br/>
  <%= form.label :purpose%>
  <%= form.text_field :purpose, value: @user.purpose %><br/>
  <%= form.label :home_price%>
  <%= form.text_field :home_price, value: @user.home_price %><br/>
  <%= form.label :down_payment%>
  <%= form.text_field :down_payment, value: @user.down_payment %><br/>
  <%= form.submit %>
<% end %>