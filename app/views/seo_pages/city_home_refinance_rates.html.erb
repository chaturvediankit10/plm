      <%(3..5).each do |i|%>
          $("#officers_mobile_data_<%=i.to_s%>").toggle();
        <%end%>
        $("#desktop_loan_off_data").hide();
    });
    $("#desktop_news").click(function(){
      <%(5..9).each do |i|%>
        $("#news_data_<%=i.to_s%>").toggle();
      <%end%>
      $("#desktop_news").hide();
    });
    $("#mobile_news_data").click(function(){
       <%(3..9).each do |i|%>
        $("#mobile_news_articles_data_<%=i.to_s%>").toggle();
      <%end%>
      $("#mobile_news_data").hide();
    })
    $(document).ready(function(){
            var docWidth = $( document ).width();
              if(docWidth > 1023 ){
                var offset = 265;
              } else {
                var offset = 105;
              }
              $('.innr-year-link').click(function(){
                var divId = $(this).attr('href');
                 $('html, body').animate({
                  scrollTop: $(divId).offset().top - offset
                }, 300);
              });
            $('.section-lft-srch-drpdwn-icn').click(function(){
              //$('.section-lft-srch-price-alert, .section-lft-srch-drpdwn-icn').hide();
              //$('.srch-results-adv-srch, #srch-edit-mb-only').show();
            });
            $('#srch-edit-mb-only').click(function(){
              $('#seo-city-mort-mb').hide();
              $('#seo-city-mort-desktop').show();
            });
            $('#para-see-more-link').click(function(){
              $('#para-see-more-link').hide();
              $('.mb-hide-para, #para-see-less-link').show();
            });
            $('#para-see-less-link').click(function(){
              $('#para-see-less-link, .mb-hide-para').hide();
              $('#para-see-more-link').show();
            });
            $('.section-lft-srch-drpdwn-icn').click(function(){
              $('.header-main').addClass("black-head");
            });
            $('#srch-edit-mb-only').click(function(){
              $('.header-main').removeClass("black-head");
            });
            });
 </script>
